<h1>Index page</h1>

<% if user_signed_in? %>
  <p>Hello, <%= current_user.username %>! Welcome back.<p>
  <p><%= link_to "Sign Out", destroy_user_session_path, :method => :delete %></p>
<% else %>
  <p>Welcome guest! Feel free to <%= link_to "Sign In", new_user_session_path %>
  or <%= link_to "Sign Up", new_user_registration_path %></p>
<% end %>

